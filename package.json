{
  "name": "zume-pwa",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "-": "0.0.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@redux-offline/redux-offline": "^2.5.2",
    "@reduxjs/toolkit": "^1.3.6",
    "@sentry/browser": "^5.15.4",
    "babel-loader": "8.1.0",
    "bottleneck": "^2.19.5",
    "cra-append-sw": "^2.7.0",
    "filesize.js": "^1.0.2",
    "firebase-tools": "^9.11.0",
    "g": "^2.0.1",
    "i18next": "^19.7.0",
    "i18next-browser-languagedetector": "^4.0.2",
    "i18next-xhr-backend": "^3.2.2",
    "jss-rtl": "^0.3.0",
    "lodash": "^4.17.15",
    "material-table": "^1.57.2",
    "navi": "^0.14.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.3.4",
    "react-navi": "^0.14.3",
    "react-redux": "^7.2.0",
    "react-scripts": "^4.0.3",
    "react-swipeable-views": "^0.13.9",
    "react-swipeable-views-utils": "^0.13.9",
    "redux-persist": "^5.10.0",
    "webpack": "4.44.2",
    "yarn": "^1.22.10"
  },
  "scripts": {
    "postinstall": "test -n \"$NO_POSTINSTALL\" || node node_modules/husky/lib/installer/bin install",
    "start": "react-scripts start",
    "build": "react-scripts build && cra-append-sw ./src/sw-custom.js && replace-in-file /%PUBLIC_URL%/g https://app.zume.training/ build/service-worker.js --isRegex",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --max-warnings=0",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "build-sw": "react-scripts build && cra-append-sw ./src/sw-custom.js && node scripts/set-public-url-in-files.js && rm -rf $npm_package_name && mv build $npm_package_name && (open -n -a 'google chrome' --args http://localhost:5000  --incognito; serve -s $npm_package_name)",
    "extract-assets": "node scripts/extract-assets-for-i18n",
    "get-asset-file-sizes": "node scripts/get-asset-file-sizes",
    "po2json": "i18next-conv",
    "sync-i18n": "node scripts/sync-i18n"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-backgrounds": "^6.2.9",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-storyshots": "^6.2.9",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^6.2.9",
    "babel-plugin-macros": "^2.7.1",
    "glob": "^7.1.6",
    "husky": "^4.2.3",
    "i18next-conv": "^9.2.1",
    "open": "^6.4.0",
    "react-test-renderer": "^16.13.1",
    "replace-in-file": "^4.1.1",
    "require-context.macro": "^1.1.0",
    "serve": "^11.1.0",
    "storybook-addon-i18next": "^0.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  }
}
